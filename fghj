set1 = set(input('').split(' ')) 
set2 = set(input('').split(' ')) 
set3 = set(input('').split(' '))
a = ''
if set1.intersection(set2, set3):
    for a in set1.intersection(set2, set3):
        print('Выбранные факультативы:',set1.intersection(set2, set3))
else:
    print("Не удалось выбрать факультативы, интересные всем троим.")
